            <Font name="Bell MT Bold" size="18.0" />
         </font>
      </TextField>
      <Button layoutX="117.0" layoutY="236.0" mnemonicParsing="false" onAction="#view" prefHeight="26.0" prefWidth="150.0" style="-fx-background-color: #7DF9FF;" text="View">
         <font>
            <Font name="Bell MT Bold" size="16.0" />
         </font>
      </Button>
      <Button layoutX="450.0" layoutY="353.0" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="110.0" style="-fx-background-color: Teal;" text="Back" textFill="#eceaea">
         <font>
            <Font name="Bell MT Bold" size="17.0" />
         </font>
      </Button>
