      <Label layoutX="240.0" layoutY="38.0" prefHeight="46.0" prefWidth="119.0" text="Ticket">
         <font>
            <Font name="Arial Black" size="31.0" />
         </font>
      </Label>
      <Label layoutX="72.0" layoutY="156.0" text="Type">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <Label layoutX="321.0" layoutY="156.0" text="Price">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <ComboBox layoutX="142.0" layoutY="159.0" onAction="#type" prefWidth="150.0" />
      <ComboBox layoutX="384.0" layoutY="159.0" onAction="#price" prefWidth="150.0" />
      <Button layoutX="375.0" layoutY="229.0" mnemonicParsing="false" onAction="#confirm" prefHeight="26.0" prefWidth="150.0" style="-fx-background-color: #7DF9FF;" text="Confirm">
         <font>
            <Font name="Bell MT Bold" size="16.0" />
